{
  "Resources": {
    "Bucket": {
      "Type": "AWS::S3::Bucket",
      "Properties": {
        "BucketName": { "Fn::Sub": [ "unusued-different-sub-${Name}", { "AnotherBucket": {"Ref" : "AnotherBucket" }} ]}
      }
    },
    "AnotherBucket": {
      "Type": "AWS::S3::Bucket",
      "Properties": {
        "BucketName": { "Fn::Sub": "unusued-bucket-fn-sub-${!Literal}" }
      }
    }
  }
}
