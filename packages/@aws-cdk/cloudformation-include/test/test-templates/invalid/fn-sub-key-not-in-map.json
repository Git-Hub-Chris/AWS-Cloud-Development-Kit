{
  "Resources": {
    "Bucket": {
      "Type": "AWS::S3::Bucket",
      "Properties": {
        "BucketName": { "Fn::Sub": [ "${Name}-${Region}-here-${AFakeResource}", { "Name": {"Ref" : "AnotherResource" }, "Region": {"Fn::Base64": "a-string" }} ]}
      }
    },
    "AnotherResource": {
      "Type": "AWS::S3::Bucket",
      "Properties": {
        "BucketName": "my-bucket"
      }
    }
  }
}
