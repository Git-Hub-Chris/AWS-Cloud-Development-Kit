{
  "PropertyTypes": {
    "patch": {
      "description": "Remove AWS::IoT::TopicRule.Timestamp, replace back with AWS::IoT::TopicRule.IotTimestamp",
      "operations": [
        {
          "op": "move",
          "from": "/AWS::IoT::TopicRule.Timestamp",
          "path": "/AWS::IoT::TopicRule.IotTimestamp"
        },
        {
          "op": "replace",
          "path": "/AWS::IoT::TopicRule.LocationAction/Properties/Timestamp/Type",
          "value": "IotTimestamp"
        }
      ]
    }
  }
}