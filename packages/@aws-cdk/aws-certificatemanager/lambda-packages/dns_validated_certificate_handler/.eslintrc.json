{
    "extends": "standard",
    "rules": {
        "semi": ["error", "always"],
        "comma-dangle": ["error", {
            "arrays": "always-multiline",
            "objects": "always-multiline",
            "imports": "never",
            "exports": "never",
            "functions": "never"
        }],
        "array-bracket-newline": ["error", {
            "multiline": true
        }]
    },
    "env": {
        "jest": true,
        "node": true
    }
}