  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@aws-cdk/assert": "^0.30.0",
    "cdk-build-tools": "^0.30.0",
    "cdk-integ-tools": "^0.30.0",
    "cfn2ts": "^0.30.0",
    "pkglint": "^0.30.0"
  },
  "dependencies": {
    "@aws-cdk/aws-ec2": "^0.30.0",
    "@aws-cdk/aws-iam": "^0.30.0",
    "@aws-cdk/aws-servicediscovery": "^0.30.0",
    "@aws-cdk/cdk": "^0.30.0"
  },
  "peerDependencies": {
    "@aws-cdk/aws-ec2": "^0.30.0",
    "@aws-cdk/aws-iam": "^0.30.0",
    "@aws-cdk/aws-servicediscovery": "^0.30.0",
    "@aws-cdk/cdk": "^0.30.0"
  },
  "engines": {
    "node": ">= 8.10.0"
  },
