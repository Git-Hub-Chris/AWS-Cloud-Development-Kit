{
 "Resources": {
  "LogGroupLambdaAuditF8F47F46": {
   "Type": "AWS::Logs::LogGroup",
   "Properties": {
    "LogGroupName": "auditDestinationForCDK",
    "RetentionInDays": 731
   },
   "UpdateReplacePolicy": "Retain",
   "DeletionPolicy": "Retain"
  },
  "LogGroupLambdaAC756C5B": {
