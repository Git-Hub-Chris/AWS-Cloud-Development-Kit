{
  "extends": "@nrwl/workspace/presets/npm.json",
  "workspaceLayout": {},
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "test",
          "lint",
          "package",
          "prepare",
          "rosetta-extract",
          "rosetta:synthetic",
          "rosetta:infuse"
        ]
      }
    }
  },
  "namedInputs": {
    "default": [
      "{projectRoot}/**/!(*.d|*.generated).ts",
      "!{workspaceRoot}/**/tsconfig.json",
      "!{workspaceRoot}/**/tsconfig.json",
      "!{workspaceRoot}/tsconfig.base.json",
      "!{workspaceRoot}/**/tsconfig.tsbuildinfo"
    ],
    "jsii": [
      "{projectRoot}/.jsii"
    ]
  },
  "targetDefaults": {
    "build": {
      "implicitDependencies": [
        "aws-cdk-lib"
      ],
      "dependsOn": [
        "^build"
      ],
      "inputs": ["default", "^default", "^jsii"],
      "outputs": [
        "!{projectRoot}/**/*.integ.*.js.snapshot/*",
        "{projectRoot}/tsconfig.json",
        "{projectRoot}/**/lib/aws-custom-resource/sdk-api-metadata.json",
        "{projectRoot}/**/build-info.json",
        "{projectRoot}/**/*.js",
        "{projectRoot}/**/*.js.map",
        "{projectRoot}/**/*.d.ts",
        "{projectRoot}/**/*.generated.ts",
        "{projectRoot}/**/built-ins.generated/*",
        "{projectRoot}/**/schema/*.json",
        "{projectRoot}/**/.jsii",
        "{projectRoot}/**/.jsii.gz",
        "{projectRoot}/**/spec/*.json",
        "{projectRoot}/**/.warnings.jsii.js"
      ]
    },
    "test": {
      "dependsOn": [
        "build"
      ],
      "inputs": ["{projectRoot}/**/*.js"],
      "outputs": [
        "{projectRoot}/coverage/*"
      ]
    },
    "rosetta-extract": {
      "dependsOn": [
        "build"
      ],
      "inputs": [
        "jsii",
        "{projectRoot}/.jsii.gz"
      ],
      "outputs": [
        "{projectRoot}/.jsii.tabl.json.gz",
        "{projectRoot}/.jsii.tabl.json",
        "{projectRoot}/.jsii",
        "{projectRoot}/.jsii.gz"
      ]
    },
    "rosetta:synthetic": {
      "dependsOn": [
        "rosetta-extract"
      ],
      "inputs": [
        "{projectRoot}/.jsii.tabl.json.gz",
        "{projectRoot}/.jsii.tabl.json",
        "{projectRoot}/.jsii",
        "{projectRoot}/.jsii.gz"
      ],
      "outputs": [
        "{projectRoot}/.jsii",
        "{projectRoot}/.jsii.gz",
        "{projectRoot}/rosetta/_generated.ts-fixture"
      ]
    },
    "rosetta:infuse": {
      "dependsOn": [
        "rosetta:synthetic"
      ],
      "inputs": [
        "{projectRoot}/.jsii",
        "{projectRoot}/.jsii.gz",
        "{projectRoot}/rosetta/_generated.ts-fixture"
      ],
      "outputs": [
        "{projectRoot}/.jsii.tabl.json",
        "{projectRoot}/.jsii.tabl.json.gz"
      ]
    },
    "package": {
      "implicitDependencies": [
        "@aws-cdk/cdk-build-tools"
      ],
      "dependsOn": ["rosetta:infuse", "build"],
      "outputs": ["{projectRoot}/dist"]
    }
  },
  "affected": {
    "defaultBase": "main"
  },
  "pluginsConfig": {
    "@nrwl/js": {
      "analyzeSourceFiles": false
    }
  }
}
