{
  "name": "cdk-build-tools",
  "private": true,
  "version": "0.7.3-beta",
  "description": "Tools package with shared build scripts for CDK packages",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/awslabs/aws-cdk"
  },
  "bin": {
    "cdk-build": "bin/cdk-build",
    "cdk-watch": "bin/cdk-watch",
    "cdk-test": "bin/cdk-test",
    "cdk-lint": "bin/cdk-lint",
    "cdk-integ": "bin/cdk-integ",
    "cdk-integ-assert": "bin/cdk-integ-assert"
  },
  "scripts": {
    "build": "tsc && tslint -p . && chmod +x bin/cdk-build && chmod +x bin/cdk-lint && chmod +x bin/cdk-test && chmod +x bin/cdk-watch && chmod +x bin/cdk-integ && chmod +x bin/cdk-integ-assert && pkglint",
    "watch": "tsc -w",
    "lint": "tslint -p . --force",
    "pkglint": "pkglint -f"
  },
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com"
  },
  "license": "LicenseRef-LICENSE",
  "devDependencies": {
    "pkglint": "^0.7.3-beta",
    "cdk-build-tools": "^0.7.3-beta"
  },
  "dependencies": {
    "@types/yargs": "^8.0.3",
    "@aws-cdk/cx-api": "^0.7.3-beta",
    "@aws-cdk/cloudformation-diff": "^0.7.3-beta",
    "aws-cdk": "^0.7.3-beta",
    "yargs": "^9.0.1"
  },
  "keywords": [
    "aws",
    "cdk"
  ]
}
